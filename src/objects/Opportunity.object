    <validationRules>
        <fullName>AfterInvitedStageCannotChangeTAT</fullName>
        <active>true</active>
        <description>After Invited Stage Cannot change TAT</description>
        <errorConditionFormula>AND(NOT( ISPICKVAL(StageName, &apos;Candidate&apos;) ),
    NOT( ISPICKVAL(StageName, &apos;Ready to Onboard&apos;) ),
    NOT( ISPICKVAL(StageName, &apos;Ready to Invite&apos;) ),
     ISCHANGED( Expected_TAT__c ) )</errorConditionFormula>
        <errorDisplayField>Expected_TAT__c</errorDisplayField>
        <errorMessage>Expected TAT cannot be changed after &apos;Ready to Invite&apos; Stage</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DelayInDaysCannotBeNone</fullName>
        <active>true</active>
        <description>On Delay Reasons selection, Delay In Days field cannot be &apos;None&apos;</description>
