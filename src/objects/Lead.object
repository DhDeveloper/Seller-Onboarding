    <validationRules>
        <fullName>Approve_Check_before_converting</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
    ISPICKVAL( Status, &quot;Converted&quot;),
    NOT(Approval_State__c = 2)
)</errorConditionFormula>
        <errorDisplayField>Approver_Comments__c</errorDisplayField>
        <errorMessage>This lead is not Approved by an Onboarding Manager yet. Pls submit for Approval if not already submitted, or request your OM to approve the same.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Basic_Data_Check_before_converting</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
 ISPICKVAL( Status, &quot;Converted&quot;),

 OR( 
   Pickup_Pincode__c = &quot;&quot;,
   LastName = &quot;&quot;,
   Company = &quot;&quot;,
   Seller_Registration_Email__c = &quot;&quot;,
   NOT(OR(ISPICKVAL( VAT_Status__c , &quot;Not Needed&quot;), 
          ISPICKVAL( VAT_Status__c , &quot;Available&quot;))),
   Are_templates_open__c = FALSE,
   Are_Brands_Approved__c = FALSE,  
   Phone = &quot;&quot;)
)</errorConditionFormula>
        <errorMessage>Please specify  pickup pincode, super category, registration email, phone, VAT Status (Not Needed or Available), Are template Open, Are Brands Approved</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Brand_Approval_before_converting</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 ISPICKVAL( Status, &quot;Converted&quot;),
 Are_Brands_Approved__c = FALSE
      
)</errorConditionFormula>
        <errorDisplayField>Are_Brands_Approved__c</errorDisplayField>
        <errorMessage>Brands must be approved before converting</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Capture_Reasons_When_Parking_Seller</fullName>
        <active>true</active>
        <errorConditionFormula>AND( OR( ISPICKVAL(Status, &apos;Parked&apos;), 
ISPICKVAL( Approver_Action__c , &apos;Parked&apos;) 
), 
OR( ISNULL(Reason_For_Parking__c), 
ISBLANK( RevisitAfterDate__c ) 
) 
)</errorConditionFormula>
        <errorDisplayField>Status</errorDisplayField>
        <errorMessage>Specify Reason(s) For Parking this seller; Select a RevisitAfterDate</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Capture_Reasons_when_Dropping_seller</fullName>
        <active>true</active>
        <errorConditionFormula>AND( OR( ISPICKVAL(Status, &apos;Dropped&apos;), 
ISPICKVAL( Approver_Action__c , &apos;Dropped&apos;) 
), 
ISBLANK(TEXT( Reason_for_Dropping__c)) 
)</errorConditionFormula>
        <errorDisplayField>Status</errorDisplayField>
        <errorMessage>Specify Reason For Dropping.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Company_before_converting</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 ISPICKVAL( Status, &quot;Converted&quot;),
 Company = &quot;&quot;
)</errorConditionFormula>
        <errorDisplayField>Company</errorDisplayField>
        <errorMessage>Company is mandatory</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EitherPickupCodeOrTempPickupShouldExist</fullName>
        <active>true</active>
        <errorConditionFormula>AND( RecordType.Name = &apos;Evaluation&apos;, Pickup_Pincode__c =&quot;&quot;,  ISBLANK( Temp_Pickup_Pincode__c ) )</errorConditionFormula>
        <errorDisplayField>Pickup_Pincode__c</errorDisplayField>
        <errorMessage>You must enter a value in either Pickup Pincode or Temp Pickup Pincode</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Last_name_before_converting</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 ISPICKVAL( Status, &quot;Converted&quot;),
 LastName = &quot;&quot;
)</errorConditionFormula>
        <errorDisplayField>LastName</errorDisplayField>
        <errorMessage>Last name is mandatory</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>On_Resubmit_Approver_Comments_Mandatory</fullName>
        <active>true</active>
        <description>On Resubmit, Approver Comments are Mandatory</description>
        <errorConditionFormula>AND( ISPICKVAL( Approver_Action__c , &apos;Resubmit&apos;), 
