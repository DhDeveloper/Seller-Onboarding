        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/apex.js&quot;)} 
try{
    var updateLeadRecords = []; 
    var message = &apos;&apos;;
    var leadSObject = new sforce.SObject(&quot;Lead&quot;);
    leadSObject.id = &apos;{!Lead.Id}&apos;; 
    leadSObject.Approval_State__c = &apos;{!Lead.Approval_State__c}&apos;;

    /*var strQuery=&quot;SELECT Pickup_Pincode__c FROM Lead where Id=&apos;{!Lead.Id}&apos;&quot;;
    var result = sforce.connection.query(strQuery);
    var records = result.getArray(&quot;records&quot;);
    leadSObject.Pickup_Pincode__c = records[0].Pickup_Pincode__c;*/
    
    leadSObject.LastName =  &apos;{!Lead.LastName}&apos;;
    leadSObject.Company =  &apos;{!Lead.Company}&apos;;
    leadSObject.Seller_Display_Name__c =  &apos;{!Lead.Seller_Display_Name__c}&apos;;
    leadSObject.Seller_Registration_Email__c =  &apos;{!Lead.Seller_Registration_Email__c}&apos;;
    leadSObject.VAT_Status__c =  &apos;{!Lead.VAT_Status__c}&apos;;
    leadSObject.Are_templates_open__c =  &apos;{!Lead.Are_templates_open__c}&apos;;
    leadSObject.Are_Brands_Approved__c =  &apos;{!Lead.Are_Brands_Approved__c}&apos;;
    leadSObject.SuperCategory__c =  &apos;{!Lead.SuperCategory__c}&apos;;
    leadSObject.Phone =  &apos;{!Lead.Phone}&apos;;

    if(leadSObject.Approval_State__c == 0){
        if(//leadSObject.Pickup_Pincode__c == &apos;&apos; ||
           leadSObject.LastName == &apos;&apos; ||
           leadSObject.Company == &apos;&apos; ||
           //leadSObject.Seller_Display_Name__c == &apos;&apos; ||
           leadSObject.Seller_Registration_Email__c == &apos;&apos; || 
           (leadSObject.VAT_Status__c != &apos;Not Needed&apos; &amp;&amp;
            leadSObject.VAT_Status__c != &apos;Available&apos;) || 
           leadSObject.Are_templates_open__c == 0 ||
           leadSObject.Are_Brands_Approved__c == 0 ||
           leadSObject.SuperCategory__c == &apos;&apos; ||
           leadSObject.Phone == &apos;&apos;)  {
               message = &apos;Please specify pickup pincode, super category, &apos;+ &apos;registration email, phone, VAT Status (Not Needed or Available),&apos;+
&apos; Are template Open, Are Brands Approved&apos;;
            alert(message);
         }
         if(message == &apos;&apos;){
         if(&apos;{!Lead.OwnerId}&apos; != &apos;{!$User.Id}&apos;){
           message = &apos;Please assign a Lead Owner before you submit for approval&apos;; 
           alert(message);
         }}
    }else if(leadSObject.Approval_State__c == 1){
        message = &apos;This Lead record already submitted for approval&apos;;
        alert(message);
    }else if(leadSObject.Approval_State__c == 2){
        message = &apos;This Lead record already approved by approver&apos;;
        alert(message);
    }else if(leadSObject.Approval_State__c == 4){
        message = &apos;This Lead record is in Dropped state and cannot be submitted for approval&apos;;
        alert(message);
    }

    if(message == &apos;&apos;){
        // make the field change to approve 
        leadSObject.Approval_State__c = 1; 
        updateLeadRecords.push(leadSObject); 
        var result = sforce.connection.update(updateLeadRecords); 
        alert(&apos;Record submitted for approval&apos;);
        window.location.reload();
    }
}catch(e){
    alert(&quot;Error: &quot;+e); 
}</url>
